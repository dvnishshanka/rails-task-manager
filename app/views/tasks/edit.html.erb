<h1>Edit Task</h1>

<%= form_with model: @task do |f| %>
<div class="container">
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <br>
  <div>
    <%= f.check_box :completed %>
    <%= f.label :completed %>
</div>
  <br>
  <%= f.submit "Update Task" %>
  </div>
<% end %>
